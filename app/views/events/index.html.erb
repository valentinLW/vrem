<div class="swiper verticalSwiper">
  <div class="swiper-wrapper">
    <div class="swiper-slide">
      <div id="events_calendar">
        <%= render "calendar", events: @events %>
      </div>
    </div>
    <div class="swiper-slide">
      <%= render "events_index", invitations: @invitations, events: @events %>
    </div>
  </div>
</div>
